    {
        "filename": "login-comp.vue",
        "filepath": "D:/vue/my-app\\src\\components\\login-comp.vue",
        "source_code": "<template>\n    <div class=\"login-container\">\n      <h1 class=\"text-2xl font-bold mb-4\">{{ isSignUp ? 'Create New Account' : 'Sign In' }}</h1>\n      <form @submit=\"handleSubmit\" class=\"flex flex-col space-y-4 animate-fade-in\">\n        <div class=\"flex flex-col\">\n          <label for=\"email\" class=\"mb-2 font-semibold\">Email:</label>\n          <input type=\"email\" id=\"email\" v-model=\"email\" required class=\"p-2 border rounded-md\" />\n        </div>\n        <div class=\"flex flex-col\">\n          <label for=\"password\" class=\"mb-2 font-semibold\">Password:</label>\n          <input type=\"password\" id=\"password\" v-model=\"password\" required class=\"p-2 border rounded-md\" />\n        </div>\n        <button type=\"submit\" class=\"p-2 bg-green-500 text-white rounded-md\">\n          {{ isSignUp ? 'Sign Up' : 'Sign In' }}\n        </button>\n        <button type=\"button\" @click=\"toggleMode\" class=\"p-2 bg-green-100 text-green-700 rounded-md\">\n          {{ isSignUp ? 'Already have an account?' : 'Create new account?' }}\n        </button>\n      </form>\n      <Notification v-if=\"errorMessage\" :message=\"errorMessage\" />\n      <div v-if=\"apiResponse\" class=\"mt-4 p-2 bg-green-100 border rounded-md animate-fade-in\">\n        <h3 class=\"font-semibold\">Response:</h3>\n        <pre>{{ apiResponse }}</pre>\n      </div>\n    </div>\n  </template>\n  \n  <script setup>\n  import { ref } from 'vue';\n  import Notification from './notification-pop.vue';\n  \n  const email = ref('');\n  const password = ref('');\n  const isSignUp = ref(false);\n  const apiResponse = ref(null);\n  const errorMessage = ref('');\n  \n  const toggleMode = () => {\n    isSignUp.value = !isSignUp.value;\n    apiResponse.value = null;\n    errorMessage.value = '';\n  };\n  \n  const handleSubmit = async (e) => {\n    e.preventDefault();\n    try {\n      const response = await fetch(isSignUp.value ? '/api/signup' : '/api/login', {\n        method: 'POST',\n        headers: { 'Content-Type': 'application/json' },\n        body: JSON.stringify({ email: email.value, password: password.value }),\n      });\n      const data = await response.json();\n      if (response.ok) {\n        apiResponse.value = data;\n        errorMessage.value = '';\n      } else {\n        apiResponse.value = null;\n        errorMessage.value = data.message || 'An error occurred. Please try again.';\n      }\n    } catch (error) {\n      apiResponse.value = null;\n      errorMessage.value = 'An error occurred. Please try again.';\n    }\n  };\n  </script>\n  \n  <style scoped>\n  .login-container {\n    max-width: 320px;\n    margin: 2rem auto;\n    padding: 1.5rem;\n    border: 1px solid #e0e0e0;\n    border-radius: 8px;\n    background-color: #f0fff4;\n    text-align: center;\n    box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1);\n  }\n  \n  @keyframes fade-in {\n    from {\n      opacity: 0;\n    }\n    to {\n      opacity: 1;\n    }\n  }\n  \n  .animate-fade-in {\n    animation: fade-in 1s ease-out;\n  }\n  </style>\n  "
    },
